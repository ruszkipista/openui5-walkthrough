          headerText="{i18n>app.list.invoices.title}"
          class="sapUiResponsiveMargin"
          width="auto">
        <items>
            <ObjectListItem title="{invoice>Quantity} x {invoice>ProductName}"/>
        </items>
    </List>
</mvc:View>